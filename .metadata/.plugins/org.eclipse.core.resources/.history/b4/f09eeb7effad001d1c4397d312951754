package test;

// 3)=> spy number sum its digits equal the product of the digits

import java.util.Scanner;

public class Test2 {

	
	public static void main(String[] args) {
		int pro=0;
		int s=0;
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		while(n!=0) {
			int d=n%10;
			pro=pro+d;
			s=s*d;
			
		}
		

	}

}
