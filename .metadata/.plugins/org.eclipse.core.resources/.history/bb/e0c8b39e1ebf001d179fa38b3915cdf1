package String_Practice;
import java.util.*;
class A1{
	Scanner sc=new Scanner(System.in);
	
	void CharSaparateLine() {
		String s=sc.nextLine();
		for(int i=0;i<s.length();i++) {
			char c=s.charAt(i);
			System.out.println(c);
		}
			
	}

	
	
	
	
}


public class Main1 {
   public static void main(String[] args) {
	   A1 a=new A1();
	   a.CharSaparateLine();
	
}

}
