package maccess;
import java.util.*;
public class Demo_String7 {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the String :");
		String str=sc.nextLine();
		int len=str.length();
		
		int ac=0;
		for(int i=0;i<=len-1;i++) {
			int ch=str.charAt(i);
			int ascii=(int)ch;
			
			switch(ch) {
			case "0" :
			case "1" :
			case "2" :
			case "3" :
			case "4" :
			case "5" :
			case "6" :
			case "7" :
			case "8" :
			case "9" :
			}
		
		}
		
	}

}
