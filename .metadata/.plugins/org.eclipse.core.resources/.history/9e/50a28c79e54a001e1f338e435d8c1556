package s1;

public class homeworkPatttern2 
{
	public static void main(String[] args) {
		
		int n=5;
		
		for(int i=1;i<=n;i++)
		{
			for(int j=1;j<=n-i;j++)
			{
				System.out.print(" ");
			}
//			System.out.println();
			
			for(int k=1;k<=n;k++)
			{
				System.out.print(k);
			}
			
			System.out.println();
		}
	}

}
