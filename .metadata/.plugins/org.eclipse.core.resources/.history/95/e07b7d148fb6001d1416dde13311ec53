package maccess;
import java.io.*;
public class FISDemo {

	public static void main(String[] args) 
			throws IOException,FileNotFoundException{
		
		FileOutputStream fis=new FileOutputStream("manu.txt");
		
		fis.write(1);
		
		
		System.out.println("Data is saved ...." );
		
		fis.close();
		
		

	}

}
