package string;



public class StringEx 
{
	public static void main(String[] args) {
		
		String s="manu india";
		StringBuffer sb=new StringBuffer();
		
		for(int i=0;i<s.length();i++)
		{
			for(char j='a';j<='z';j++)
			{
				if((s.charAt(i)==j))
				{
					System.out.println( sb.append(j));
				}
			}
		}
	}

}
