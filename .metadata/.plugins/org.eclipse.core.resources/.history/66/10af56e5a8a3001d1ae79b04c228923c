package test;
public class Arrays_Problems {
	public static void main(String[] args) {
		int count = 0;
		long sum=0;
		int multi = 0;
		int[] a = { 12, 23, 34, 12, 23, 44, 35 };
		for (int i=1;i <=a.length;i++) {
			sum=sum+a[i];
		}
		System.out.println("sum :" + sum);
                                                                 //		System.out.println("multiplication : " + multi);

	}

}














