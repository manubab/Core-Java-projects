package test;
public class Arrays_Problems {
	public static void main(String[] args) {
		int count=0;int c=0;
		int[] a= {12,23,34};
		for(int i=0;i<=a.length-1;i++) {
                                                // 12*23*34=9384	                        
			count=a[i];
			c=a[i]*i;
			
			
		}
		System.out.println("sum"+count);
		System.out.println(c);
	}

}

