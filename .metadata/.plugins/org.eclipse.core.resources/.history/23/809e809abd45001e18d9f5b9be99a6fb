package string;

import java.util.Scanner;

public class Test1 
{
	
	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		
		
		
		String s="Manu";
		
		char c=s.charAt(1);
		
		int n=c;
		
		System.out.println(n);
		
		c=n+2;
		
		System.out.println(c);
		
		
		
		
	}

}
