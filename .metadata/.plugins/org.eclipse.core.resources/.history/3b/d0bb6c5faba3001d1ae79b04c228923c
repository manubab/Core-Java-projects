package test;

public class Split_String {
	public static void main(String[] args) {
		String s="i am a java devoloper";
		int count=1;
		for(int i=0;i<=s.length()-1;i++) {
			
			if((s.charAt(i)==' ')&&(s.charAt(i)!=0)) {
				count++;
			}
			
			}
		System.out.println("The number of counts"+count);
		
	}
	
}
