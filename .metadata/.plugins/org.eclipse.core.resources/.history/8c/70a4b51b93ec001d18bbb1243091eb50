package practice;
import java.util.stream.*;
import java.util.*;
public class Demo1 {
	public static void main(String[] args) {
		
		String[] arr={"manu","sai","amma","rajan"};
		
		Stream<String> ob=Stream.of(arr);
		
		Iterator<String> it=ob.iterator();
		
		while(it.hasNext())
		{
			System.out.println(it.next());
		}
	}

}
