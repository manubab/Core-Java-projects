package String_Practice;
import java.io.*;
public class Test {
	public static void main(String[] args) {
		String a="I am a Indian I am love indian";
		int n=a.length();
		String w="";
		String nw="";
		for(int i=0;i<n;i++)
		{
			char c=a.charAt(i);
			if(c==' ')
			{
				w=w+c;
				System.out.println(w);
			}else {
				
			nw="";
			}
		}
		
		
		
		
		
		
	}

}
